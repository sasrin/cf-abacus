{
  "name": "cf-abacus",
  "description": "CF Abacus usage metering and aggregation service",
  "license": "Apache-2.0",
  "readme": "README.md",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://github.com/cloudfoundry-incubator/cf-abacus",
  "bugs": {
    "url": "https://github.com/cloudfoundry-incubator/cf-abacus/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cloudfoundry-incubator/cf-abacus.git"
  },
  "keywords": [
    "cf",
    "abacus"
  ],
  "files": [
    ".eslintrc",
    ".jshintrc",
    ".npmrc",
    ".travis.yml",
    "LICENSE",
    "NOTICE",
    "bin/",
    "demo/",
    "doc/",
    "etc/",
    "lib/",
    "test/",
    "tools/"
  ],
  "scripts": {
    "bootstrap": "etc/npm2 install etc && npm2 install tools/babel && npm2 install tools/publish",
    "babel": "foreach abacus :path npm run babel",
    "build": "etc/versions && npm run bootstrap && npm run babel && npm2 install && npm test",
    "clean": "git clean -df",
    "rebuild": "npm run clean && npm run build",
    "lint": "foreach abacus node_modules/:name npm run lint",
    "hint": "foreach abacus node_modules/:name jshint *.js test/*.js",
    "test": "foreach abacus node_modules/:name npm test && coverage",
    "coveralls": "cat .coverage/lcov.info | coveralls",
    "doc": "cat README.md | mddoc >README.html",
    "start": "bin/start",
    "stop": "bin/stop",
    "demo": "bin/demo",
    "perf": "bin/perf",
    "pub": "npm run babel && foreach abacus :path npm run pub && publish",
    "cfpush": "bin/cfpush",
    "cfstart": "bin/cfstart",
    "cfstop": "bin/cfstop"
  },
  "dependencies": {
    "abacus-service-config": "file:lib/config/service",
    "abacus-price-config": "file:lib/config/price",
    "abacus-usage-accumulator": "file:lib/aggregation/accumulator",
    "abacus-usage-aggregator": "file:lib/aggregation/aggregator",
    "abacus-instance-launcher": "file:lib/aggregation/launcher",
    "abacus-aggregation-db": "file:lib/aggregation/db",
    "abacus-usage-reporting": "file:lib/aggregation/reporting",
    "abacus-usage-collector": "file:lib/metering/collector",
    "abacus-usage-meter": "file:lib/metering/meter",
    "abacus-metering-db": "file:lib/metering/db",
    "abacus-metering-schemas": "file:lib/metering/schemas",
    "abacus-usage-rate": "file:lib/rating/rate",
    "abacus-account-stub": "file:lib/stubs/account",
    "abacus-provisioning-stub": "file:lib/stubs/provisioning",
    "abacus-batch": "file:lib/utils/batch",
    "abacus-breaker": "file:lib/utils/breaker",
    "abacus-cluster": "file:lib/utils/cluster",
    "abacus-dbclient": "file:lib/utils/dbclient",
    "abacus-dbserver": "file:lib/utils/dbserver",
    "abacus-debug": "file:lib/utils/debug",
    "abacus-events": "file:lib/utils/events",
    "abacus-express": "file:lib/utils/express",
    "abacus-hystrix": "file:lib/utils/hystrix",
    "abacus-lock": "file:lib/utils/lock",
    "abacus-lrudown": "file:lib/utils/lrudown",
    "abacus-partition": "file:lib/utils/partition",
    "abacus-perf": "file:lib/utils/perf",
    "abacus-request": "file:lib/utils/request",
    "abacus-retry": "file:lib/utils/retry",
    "abacus-router": "file:lib/utils/router",
    "abacus-schema": "file:lib/utils/schema",
    "abacus-seqid": "file:lib/utils/seqid",
    "abacus-throttle": "file:lib/utils/throttle",
    "abacus-transform": "file:lib/utils/transform",
    "abacus-urienv": "file:lib/utils/urienv",
    "abacus-vcapenv": "file:lib/utils/vcapenv",
    "abacus-webapp": "file:lib/utils/webapp",
    "abacus-yieldable": "file:lib/utils/yieldable",
    "abacus-perf-test": "file:test/perf",
    "abacus-demo-client": "file:demo/client",
    "abacus-doc": "file:doc",
    "abacus-etc": "file:etc",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "abacus-babel": "file:tools/babel",
    "abacus-coverage": "file:tools/coverage",
    "abacus-eslint": "file:tools/eslint",
    "abacus-mddoc": "file:tools/mddoc",
    "abacus-mocha": "file:tools/mocha",
    "abacus-packapp": "file:tools/packapp",
    "abacus-publish": "file:tools/publish",
    "coveralls": "^2.11.3",
    "jshint": "^2.8.0",
    "levelup": "~0.18.4",
    "pouchdb": "3.6.0"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=2.0.0"
  }
}
